<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="sol_example">
	<typeAlias alias="ExampleDTO" type="kr.co.solproject.example.ExampleDTO" />
	
		<update id="insert" parameterClass="ExampleDTO">
			insert into sol_question(questno, example)
			values(#questno#, #example#)
		</update>
	
</sqlMap>